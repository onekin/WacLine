{
  "presets": [[
    "@babel/preset-env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "useBuiltIns": "usage",
      "corejs": {
        "version": 3
      },
      "debug": false
    }]],
    "plugins": [
     ["@babel/plugin-proposal-class-properties"],
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-syntax-async-generators"
  ],
  "ignore": ["node_modules"],
  "retainLines": true,
  "sourceType": "unambiguous"
}
